								"referenceName": "DataFlowDemo1",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"DataLake": {},
									"SQLDataBaseSink": {},
									"sink1": {},
									"sink2": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
