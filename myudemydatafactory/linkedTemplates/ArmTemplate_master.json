{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myudemydatafactory"
		},
		"AzureDestinationDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureDestinationDB'"
		},
		"AzureSQLSampleDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLSampleDB'"
		},
		"AzureSourceDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSourceDB'"
		},
		"AzureSourceDB2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSourceDB2'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"udemylake1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'udemylake1'"
		},
		"DataFlowLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DataFlowLinkedService'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://MyUdemyDefaultKeyVault.vault.azure.net/"
		},
		"FlatFileLinkedService_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\source\\Azure\\Files"
		},
		"FlatFileLinkedService_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "AzureLocalUser"
		},
		"GetMetaDataLinkedService_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\source\\Azure\\Files"
		},
		"GetMetaDataLinkedService_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "AzureLocalUser"
		},
		"udemylake1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://udemylake1.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "myudemydatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDestinationDB_connectionString": {
						"value": "[parameters('AzureDestinationDB_connectionString')]"
					},
					"AzureSQLSampleDB_connectionString": {
						"value": "[parameters('AzureSQLSampleDB_connectionString')]"
					},
					"AzureSourceDB_connectionString": {
						"value": "[parameters('AzureSourceDB_connectionString')]"
					},
					"AzureSourceDB2_connectionString": {
						"value": "[parameters('AzureSourceDB2_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"udemylake1_accountKey": {
						"value": "[parameters('udemylake1_accountKey')]"
					},
					"DataFlowLinkedService_connectionString": {
						"value": "[parameters('DataFlowLinkedService_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"udemylake1_properties_typeProperties_url": {
						"value": "[parameters('udemylake1_properties_typeProperties_url')]"
					},
					"FlatFileLinkedService_properties_typeProperties_host": {
						"value": "[parameters('FlatFileLinkedService_properties_typeProperties_host')]"
					},
					"FlatFileLinkedService_properties_typeProperties_userId": {
						"value": "[parameters('FlatFileLinkedService_properties_typeProperties_userId')]"
					},
					"GetMetaDataLinkedService_properties_typeProperties_host": {
						"value": "[parameters('GetMetaDataLinkedService_properties_typeProperties_host')]"
					},
					"GetMetaDataLinkedService_properties_typeProperties_userId": {
						"value": "[parameters('GetMetaDataLinkedService_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "myudemydatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'myudemydatafactory_ArmTemplate_0')]"
			]
		}
	]
}